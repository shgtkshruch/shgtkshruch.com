section.section.history.js-heading#history(data-speed=0)
  .heading
    .heading__main.heading__typed
      span
        span.heading__title history
        br
        span.heading__sub Learn WEB technology with Internet.
    h2.heading__typed.js-heading-output
  ol.history__list
    - var history = [];
    - history[0] = {age: 2011, name: 'dotinstall', url: 'http://dotinstall.com/users/shgtkshruch', text: ['HTML・CSS・JavaScriptなどWEBテクノロジーの基礎を学ぶ。', '動画完了数は1500本以上。']};
    - history[1] = {age: 2013, name: 'Schoo', url: 'https://schoo.jp/', text: ['課題制作で実際にWEBサイトを制作。', '授業内のコンペティションで最優秀賞を受賞。']};
    - history[2] = {age: 2015, name: 'Lancers', url: 'https://www.lancers.jp/profile/sh19e', text: ['コーポレートサイトやランディングページなどの制作を受注。', '全ての案件の評価で☆5つを獲得。']};
    - history[3] = {age: 2016, name: 'Chogenbashugi', url: 'http://chogenba.com/', text: ['WEB制作会社の案件を元に実践的なコーディング技術を学ぶ。']};

    each his in history
      li.history__item
        .history__header
          span.histroy__year= his.age
          span.history__name.link.js-history-name= his.name
        p.history__text.jp
          each text in his.text
            span.text
              span.text__content= text
            br
          span.text
            span.text__content
              a.history__btn.link(href=`${his.url}` target="_blank") more

